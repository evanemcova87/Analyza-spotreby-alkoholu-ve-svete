CREATE OR REPLACE TABLE l2_final_table_male_female AS

SELECT COA.COUNTRY_CODE
     , COA.COUNTRY_NAME
     , COA.YEAR
     , COA.SEX
     , COA.REGION
     , COA.SUB_REGION
     , COA.CONSUMPTION
     , ADS.ALCOHOL_DEATHS

FROM L1_CONSUMPTION_OF_ALCOHOL COA

LEFT JOIN L1_ALCOHOL_DEATHS_SEXES ADS
ON COA.COUNTRY_CODE = ADS.COUNTRY_CODE
AND COA.YEAR = ADS.YEAR
AND COA.SEX = ADS.SEX

WHERE COA.SEX IN ('Both sexes', 'Male', 'Female')
;
